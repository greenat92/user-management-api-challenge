  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:db-setup": "ts-node test/setup/database.setup.ts",
    "test": "tsc && npm run test:db-setup && cross-env APP_ENV=test jest --forceExit",
    "test:unit": "tsc && cross-env APP_ENV=test TEST_TYPE=UNIT jest --forceExit",
    "test:smoke": "tsc && npm run test:db-setup && cross-env APP_ENV=test TEST_TYPE=SMOKE jest --forceExit",
    "test:e2e": "tsc && npm run test:db-setup && cross-env APP_ENV=test TEST_TYPE=E2E jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate:generate": "typeorm migration:generate -n",
    "migrate:run": "typeorm migration:run",
    "migrate:revert": "typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
